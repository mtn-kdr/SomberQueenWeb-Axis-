@model IEnumerable<SomberQueenWeb_Axis_.Models.EncryptedFile>

@{
    ViewData["Title"] = "Encrypted Files";
}

<h1>Encrypted Files</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var file in Model)
        {
            <tr>
                <td>@file.id</td>
                <td>@file.username</td>
                <td>
                    <a asp-action="Details" asp-route-id="@file.id">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>
